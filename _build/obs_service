    <service name="download_url">
        <param name="protocol">https</param>
        <param name="host">raw.githubusercontent.com</param>
        <param name="port">443</param>
        <param name="path">/ConTextMe/digest/master/_build/digest.spec</param>
    </service>

    <service name="obs_scm">
        <param name="url">https://github.com/ConTextMe/digest.git</param>
        <param name="revision">master</param>
        <param name="scm">git</param>
        <param name="versionformat">__Ver@@PARENT_TAG@__RelDate@%cd__Hash@git+%h__.obs</param>
        <param name="exclude">.git</param>  
        <param name="versionrewrite-pattern">v(.*)</param>
        <param name="changesgenerate">enable</param>
    </service>
</services>
